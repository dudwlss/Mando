#include <stdio.h>

int main() {
    int x, y;

    printf("x 좌표를 입력하세요: ");
    scanf("%d", &x);

    printf("y 좌표를 입력하세요: ");
    scanf("%d", &y);

    if (x > 0 && y > 0) {
        printf("(%d, %d)는 1사분면에 속합니다.\n", x, y);
    } else if (x < 0 && y > 0) {
        printf("(%d, %d)는 2사분면에 속합니다.\n", x, y);
    } else if (x < 0 && y < 0) {
        printf("(%d, %d)는 3사분면에 속합니다.\n", x, y);
    } else if (x > 0 && y < 0) {
        printf("(%d, %d)는 4사분면에 속합니다.\n", x, y);
    } else {
        printf("(%d, %d)는 원점에 위치합니다.\n", x, y);
    }

    return 0;
}
